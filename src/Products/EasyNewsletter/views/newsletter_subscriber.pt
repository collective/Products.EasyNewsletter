<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="Products.EasyNewsletter"
      metal:use-macro="context/main_template/macros/master">
<body>
  <metal:block fill-slot="content-core">
    <div tal:content="structure view/w/salutation/render" />
    <div tal:content="structure view/w/name_prefix/render" />
    <div tal:content="structure view/w/firstname/render" />
    <div tal:content="structure view/w/lastname/render" />
    <div tal:content="structure view/w/organization/render" />
    <div tal:content="structure view/w/email/render" />
  </metal:block>
</body>
</html>
